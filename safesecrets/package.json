{
  "name": "safesecrets",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "buildpreview": "tsc && vite build --mode preview",
    "copy_pwa": "copyfiles -u 2 \"./src/pwa/*\" ./dist/",
    "copy_logo": "copyfiles -u 2 \"./src/images/Logo512.png\" ./dist/assets/src/images/",
    "copy_assets": "npm run copy_pwa & npm run copy_logo",
    "publish": "npm run build && npm run copy_assets",
    "preview": "npm run buildpreview && npm run copy_assets"
  },
  "dependencies": {
    "gapi-script": "^1.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-login": "^5.2.2",
    "react-jwt": "^1.1.7",
    "react-router-dom": "^6.4.2",
    "sweetalert2": "^11.6.14",
    "sweetalert2-react-content": "^4.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/gapi": "^0.0.43",
    "@types/gapi.auth2": "^0.0.56",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vitejs/plugin-react": "^1.0.7",
    "copyfiles": "^2.4.1",
    "eslint": "^8.28.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "sass": "^1.55.0",
    "typescript": "^4.5.4",
    "vite": "^2.8.0"
  }
}
